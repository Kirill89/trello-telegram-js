{
  "name": "trello-telegram-js",
  "version": "1.0.0",
  "description": "Telegram bot server to notify about deadlines in Trello",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint .",
    "start": "node index.js",
    "forever": "pm2 start index.js --instances 1 --name trello-telegram-js --log app.log",
    "restart": "pm2 restart trello-telegram-js"
  },
