  "license": "MIT",
  "dependencies": {
    "bluebird": "~3.4.0",
    "got": "~6.3.0",
    "js-yaml": "~3.6.1",
    "lodash": "~4.13.1",
    "moment": "~2.13.0",
    "nedb": "~1.8.0",
    "node-telegram-bot-api": "~0.23.3"
  },
  "devDependencies": {
    "eslint": "~6.1.0"
  }
}
